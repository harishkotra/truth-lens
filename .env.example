# DKG Configuration
# Your wallet private key for signing DKG transactions
PRIVATE_KEY=0xYOUR_PRIVATE_KEY_HERE

# Your wallet public address
PUBLIC_KEY=0xYOUR_PUBLIC_ADDRESS_HERE

# DKG API endpoint (default: localhost)
OT_NODE_HOSTNAME=http://localhost

# DKG API port (NOT the node engine port!)
# Default: 9200 for DKG API
DKG_PORT=9200

# Blockchain network ID
# otp:20430 = NeuroWeb Testnet
# otp:2043 = NeuroWeb Mainnet
BLOCKCHAIN_ID=otp:20430

# Gaia/LLM Configuration
# URL to your Gaia node or compatible LLM endpoint
LLM_URL=https://YOUR_GAIA_NODE_URL/v1/chat/completions

# LLM model identifier
LLM_MODEL=gpt-oss-20b-MXFP4_MOE

# Demo Mode (set to true to skip DKG publishing for testing)
DEMO_MODE=false

# Server Configuration
PORT=3000
NODE_ENV=development